<div>
  {{#each randomBeer}}
  <img src="{{this.image_url}}" alt="{{this.name}}" />
  <h4>{{this.name}}</h4>
  <p>{{this.tagline}}</p>
  <p>{{this.description}}</p>
  <h3>Food Pairing</h3>
  {{#each food_pairing }}
  <ul>
    <li>{{this}}</li>
  </ul>
  {{/each}}
  <p><strong>Brewer tips:</strong> {{this.brewers_tips}}</p>
  {{/each}}
</div>